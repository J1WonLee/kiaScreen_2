<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="plus"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="rotationPlus"
                android:pivotX="12"
                android:pivotY="12"
                android:rotation="0.0">
                <path
                    android:name="first"
                    android:pathData="M 11,21 L 11,3 L 13,3 L 13,21 Z"
                    android:fillColor="@color/black" />
            </group>

            <group
                android:name="rotationMinus"
                android:pivotX="12"
                android:pivotY="12"
                android:rotation="0.0">
                <path
                    android:name="second"
                    android:pathData="M 3,13 L 3,11 L 21,11 L21,13 Z"
                    android:fillColor="@color/black" />
            </group>

        </vector>
    </aapt:attr>
    <target android:name="rotationPlus">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="300"
                android:propertyName="rotation"
                android:valueFrom="0.0"
                android:valueTo="90.0" />
        </aapt:attr>
    </target>
</animated-vector>

