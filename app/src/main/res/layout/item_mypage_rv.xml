<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/mypage_rv_wrapper"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    android:paddingTop="20dp"
    android:paddingLeft="30dp"
    android:paddingRight="30dp"
    android:paddingBottom="20dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_gravity="center_vertical"
    android:layout_width="130dp"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/mypage_rv_cnt_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:fontFamily="@font/musinsabold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/mypage_rv_content_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="모델\n비교"
        android:fontFamily="@font/musinsamedium"
        android:textColor="@color/gray_txt"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/mypage_rv_cnt_tv"
        app:layout_constraintLeft_toLeftOf="@+id/mypage_rv_cnt_tv"
        app:layout_constraintRight_toRightOf="@+id/mypage_rv_cnt_tv" />

</androidx.constraintlayout.widget.ConstraintLayout>