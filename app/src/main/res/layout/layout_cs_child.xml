<?xml version="1.0" encoding="utf-8"?>
<com.copy.kiascreen.custom.MenuWrapperLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/child_item_wrapper"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <TextView
        android:id="@+id/first_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/kialight"
        android:text="- 고객센터 안내 "
        android:textSize="15sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/second_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/kialight"
        android:text="- 1:1 문의 "
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/first_menu"
        app:layout_constraintLeft_toLeftOf="@+id/first_menu" />

    <TextView
        android:id="@+id/third_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/kialight"
        android:text="- FAQ "
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/second_menu"
        app:layout_constraintLeft_toLeftOf="@+id/second_menu" />

    <TextView
        android:id="@+id/fourth_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/kialight"
        android:text="- 칭찬 알림방 "
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/third_menu"
        app:layout_constraintLeft_toLeftOf="@+id/third_menu" />

    <TextView
        android:id="@+id/fifth_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/kialight"
        android:text="- 하자 재발 통보 "
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/fourth_menu"
        app:layout_constraintLeft_toLeftOf="@+id/fourth_menu" />

</com.copy.kiascreen.custom.MenuWrapperLayout>