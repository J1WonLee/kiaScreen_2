<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/fund_first_column_view"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/fund_first_column_tv"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="종류"
        android:textColor="@color/white"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/fund_first_column_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_first_column_view"
        app:layout_constraintRight_toRightOf="@+id/fund_first_column_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_first_column_view" />

    <View
        android:id="@+id/fund_second_column_view"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@drawable/border_bottom_transparent"
        app:layout_constraintTop_toBottomOf="@+id/fund_first_column_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_first_column_view" />

    <TextView
        android:id="@+id/fund_second_column_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="도시철도채권\n(지하철 공채)"
        android:textColor="@color/black"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/fund_second_column_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_second_column_view"
        app:layout_constraintRight_toRightOf="@+id/fund_second_column_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_second_column_view" />

    <View
        android:id="@+id/fund_third_column_view"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@drawable/border_bottom_transparent"
        app:layout_constraintTop_toBottomOf="@+id/fund_second_column_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_first_column_view" />

    <TextView
        android:id="@+id/fund_third_column_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="지역개발채권"
        android:textColor="@color/black"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/fund_third_column_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_third_column_view"
        app:layout_constraintRight_toRightOf="@+id/fund_third_column_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_third_column_view" />

    <View
        android:id="@+id/fund_second_row_view"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/fund_first_column_view" />

    <TextView
        android:id="@+id/fund_second_row_tv"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="지역"
        android:textColor="@color/white"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/fund_second_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_second_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_second_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_second_row_view" />

    <View
        android:id="@+id/fund_third_row_view"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_first_column_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_first_column_view" />

    <TextView
        android:id="@+id/fund_third_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="서울"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_third_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_third_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_third_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_third_row_view" />

    <View
        android:id="@+id/fund_fourth_row_view"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_third_row_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_first_column_view" />

    <TextView
        android:id="@+id/fund_fourth_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="서울"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_fourth_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_fourth_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_fourth_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_fourth_row_view" />

    <View
        android:id="@+id/fund_fifth_row_view"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_fourth_row_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_first_column_view" />

    <TextView
        android:id="@+id/fund_fifth_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="서울"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_fifth_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_fifth_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_fifth_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_fifth_row_view" />

    <View
        android:id="@+id/fund_sixth_row_view"
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/fund_second_row_view" />

    <TextView
        android:id="@+id/fund_sixth_row_tv"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="상환 방식"
        android:textColor="@color/white"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/fund_sixth_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_sixth_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_sixth_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_sixth_row_view" />

    <View
        android:id="@+id/fund_seventh_row_view"
        android:layout_width="230dp"
        android:layout_height="150dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_sixth_row_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_third_row_view" />

    <TextView
        android:id="@+id/fund_seventh_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="7년 거치 후 일시 상환"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_seventh_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_seventh_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_seventh_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_seventh_row_view" />

    <View
        android:id="@+id/fund_eighth_row_view"
        android:layout_width="230dp"
        android:layout_height="50dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_third_row_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_fourth_row_view" />

    <TextView
        android:id="@+id/fund_eighth_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="5년 거치 후 일시 상환"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_eighth_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_eighth_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_eighth_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_eighth_row_view" />

    <View
        android:id="@+id/fund_ninth_row_view"
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_fourth_row_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_fifth_row_view" />

    <TextView
        android:id="@+id/fund_ninth_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="5년 거치 후 일시 상환"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_ninth_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_ninth_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_ninth_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_ninth_row_view" />


    <View
        android:id="@+id/fund_ten_row_view"
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/fund_sixth_row_view" />

    <TextView
        android:id="@+id/fund_ten_row_tv"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="비고"
        android:textColor="@color/white"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/fund_ten_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_ten_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_ten_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_ten_row_view" />

    <View
        android:id="@+id/fund_11_row_view"
        android:layout_width="230dp"
        android:layout_height="150dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_sixth_row_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_seventh_row_view" />

    <TextView
        android:id="@+id/fund_11_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="연 2.0%\n5년간 1년 단위로 복리\n나머지 2년은 단리로 계산"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_11_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_11_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_11_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_11_row_view" />

    <View
        android:id="@+id/fund_12_row_view"
        android:layout_width="230dp"
        android:layout_height="50dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_third_row_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_eighth_row_view" />

    <TextView
        android:id="@+id/fund_12_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="연 2.0 % 1년 단위 복리"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_12_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_12_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_12_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_12_row_view" />

    <View
        android:id="@+id/fund_13_row_view"
        android:layout_width="230dp"
        android:layout_height="80dp"
        android:background="@drawable/border_left_bottom_trans"
        app:layout_constraintTop_toBottomOf="@+id/fund_fourth_row_view"
        app:layout_constraintLeft_toRightOf="@+id/fund_ninth_row_view" />

    <TextView
        android:id="@+id/fund_13_row_tv"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="연 2.0 % 1년 단위 복리"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@+id/fund_13_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/fund_13_row_view"
        app:layout_constraintRight_toRightOf="@+id/fund_13_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/fund_13_row_view" />







</androidx.constraintlayout.widget.ConstraintLayout>