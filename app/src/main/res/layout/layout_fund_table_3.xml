        android:layout_width="230dp"
        android:layout_height="70dp"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/aq_first_row_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="차종"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="@+id/aq_first_row_view"
        app:layout_constraintBottom_toBottomOf="@+id/aq_first_row_view"
        app:layout_constraintLeft_toLeftOf="@+id/aq_first_row_view"
        app:layout_constraintRight_toRightOf="@+id/aq_first_row_view" />

