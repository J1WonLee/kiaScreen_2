        android:textSize="15sp"
        android:fontFamily="@font/kiab"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@+id/aq_first_column_view"
        app:layout_constraintBottom_toBottomOf="@+id/aq_first_column_view"
        app:layout_constraintLeft_toLeftOf="@+id/aq_first_column_view"
        app:layout_constraintRight_toRightOf="@+id/aq_first_column_view" />

    <View
        android:id="@+id/aq_fourth_column_view"
