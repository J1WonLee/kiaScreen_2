        android:layout_width="230dp"
        android:layout_height="70dp"
        android:background="@drawable/border_left_black"
        app:layout_constraintTop_toTopOf="@+id/aq_first_row_view"
        app:layout_constraintLeft_toRightOf="@+id/aq_first_row_view" />

    <TextView
